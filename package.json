{
  "name": "react-i18next-phraseapp",
  "version": "2.0.3",
  "description": "DEPRECATED ⛔️ Connect react-i18next with the Phrase In-Context Editor",
  "homepage": "https://phrase.com",
  "main": "./dist/react-i18next-phraseapp.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "//postshrinkwrap": "based on https://github.com/rogeriochaves/npm-force-resolutions/issues/10",
    "postshrinkwrap": "npx --quiet npm-force-resolutions && npm install --no-package-lock --no-audit --no-fund --no-progress --silent",
    "clean": "rm -rf dist/",
    "dist": "npm run clean && webpack-cli --mode production",
    "test": "jest",
    "prepublishOnly": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phrase/react-i18next-phraseapp.git"
  },
  "keywords": [
    "deprecated",
    "phrase",
    "phraseapp",
    "i18n",
    "l10n",
    "react",
    "i18next",
    "localization",
    "translation"
  ],
  "resolutions": {
    "minimist": "0.2.1"
  },
  "author": "Phrase <support@phrase.com> (https://phrase.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phrase/react-i18next-phraseapp/issues",
    "email": "support@phrase.com"
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^15.6.25",
    "@types/react-dom": "^0.14.23",
    "@types/react-i18next": "^1.7.12",
    "@types/react-test-renderer": "^15.5.8",
    "hoist-non-react-statics": "^1.2.0",
    "i18next": "^7.1.3",
    "jest": "^26.0.1",
    "lodash": ">=4.17.11",
    "merge": ">=1.2.1",
    "npm-force-resolutions": "0.0.3",
    "prop-types": "^15.7.2",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-i18next": "^2.2.3",
    "react-test-renderer": "^15.6.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^5.4.5",
    "typescript": "^3.9.3",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "i18next": ">=7.1.3",
    "react": ">=15.4.2",
    "react-i18next": ">=8.0.0"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
